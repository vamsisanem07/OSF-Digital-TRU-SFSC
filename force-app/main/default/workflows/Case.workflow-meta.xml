    <rules>
        <fullName>Assign Case From All Levels - Monitor Queue to Email English Queue</fullName>
        <active>true</active>
        <description>When Language Preference = &apos;EN&apos; &amp; Owner = All Levels - Monitor Queue &amp; Queue Sub Type = Price Match, Carrier Claim and Return Label - HWP, reassignment time is 48 hours</description>
        <formula>AND(ISPICKVAL(Account.Language_Preference__c, &apos;EN&apos;),     OR(ISPICKVAL(Queue_Sub_type__c, &apos;Price Match&apos;), 	   ISPICKVAL(Queue_Sub_type__c, &apos;Carrier claim (not Canada Post)&apos;), 	   ISPICKVAL(Queue_Sub_type__c, &apos;Carrier claim (Canada Post)&apos;), 	   ISPICKVAL(Queue_Sub_type__c, &apos;Return Label - HWP&apos;)     ),     ISPICKVAL( Monitor_Queue__c, &apos;All Levels - Monitor Queue&apos;) )</formula>
        <triggerType>onCreateOrTriggeringUpdate</triggerType>
        <workflowTimeTriggers>
            <actions>
                <name>Reset_Monitor_Queue</name>
                <type>FieldUpdate</type>
            </actions>
            <actions>
                <name>Update_Owner_To_Email_English</name>
                <type>FieldUpdate</type>
            </actions>
            <offsetFromField>Case.Assignment_Timestamp_Monitor_Queue__c</offsetFromField>
            <timeLength>48</timeLength>
            <workflowTimeTriggerUnit>Hours</workflowTimeTriggerUnit>
        </workflowTimeTriggers>
    </rules>
